{
    "id": "homebrew",
    "version": "1.0.0",
    "name": "Homebrew",
    "description": "Install homebrew",
    "options": {},
    "containerEnv": {
        "PATH": "/opt/homebrew/bin:${PATH}"
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ],
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.defaultProfile.linux": "fish"
            }
        }
    }
}